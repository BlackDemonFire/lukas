{
  "name": "lukasbot",
  "version": "1.0.0",
  "dependencies": {
    "discord-api-types": "^0.37.101",
    "discord.js": "^14.16.2",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "postgres": "^3.4.4",
    "random-org": "^2.2.0",
    "recursive-readdir": "^2.2.3",
    "systeminformation": "^5.23.5",
    "winston": "^3.14.2",
    "wmic": "^1.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.7.2",
    "@types/recursive-readdir": "^2.2.4",
    "drizzle-kit": "^0.24.2",
    "esbuild": "^0.24.0",
    "eslint": "^9.11.1",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0"
  },
  "scripts": {
    "build": "esbuild  src/**/*.ts src/*.ts --outdir=dist --format=esm --platform=node --tsconfig=tsconfig.json --minify --sourcemap",
    "start": "node --enable-source-maps -r dotenv/config dist/index.js",
    "watch:ts": "tsc --watch",
    "check": "tsc --noEmit",
    "lint": "eslint src",
    "generate": "drizzle-kit generate",
    "migrate": "drizzle-kit migrate"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  }
}
